---

// IMPORTS COMMON ASTRO COMPONENTS
import Footer from "@/components/common/footer/index.astro"
import Header from "@/components/common/header/index.astro"
import HeaderBanner from "@/components/common/header-banner/index.astro"

// IMPORTS ASTRO COMPONENTS
import AcceleratingLeaders from "@/components/home/case-studies/index.astro"
import Advantage from "@/components/home/advantage/index.astro"
import Carousel from "@/components/home/Carousel"
import Clients from "@/components/home/clients/index.astro"
import Hero from "@/components/home/index.astro"
import MoreServices from "@/components/home/more-services/index.astro"
import OurProcess from "@/components/home/our-process/index.astro"
import Services from "@/components/home/services/index.astro"
import ViewWork from "@/components/home/view-work/index.astro"
import Work from "@/components/home/work/index.astro"

// GET ASTRO PROPS
const {

    advantages,
    CDN,
    ENVIRONMENT,
    home_data,
    PROJECT,
    services,
    testimonials,
    work

} = Astro.props

---
<HeaderBanner
    ENVIRONMENT={ ENVIRONMENT }
/>
<Header
    client:load
/>
<Hero
    data={ home_data.hero }
/>
<Work
    carousel_one={ home_data.work_carousel_one }
    carousel_three={ home_data.work_carousel_three }
    carousel_two={ home_data.work_carousel_two }
    CDN={ CDN }
    PROJECT={ PROJECT }
/>
<!-- <Carousel
    client:load
    data={ home_data.work_carousel_one }
    showArrows={ true }
    autoPlay={ true }
    showDots={ true }
/> -->
<Services
    data={ services }
    services={ home_data.service }
/>
<MoreServices
    data={ home_data.more_services }
/>
<Advantage
    advantage={ home_data.advantage }
    data={ advantages }
/>
<OurProcess
    data={ home_data.our_process }
/>
<AcceleratingLeaders
    CDN={ CDN }
    data={ work }
    PROJECT={ PROJECT }
/>
<ViewWork
    data={ home_data.our_work }
/>
<Clients
    CDN={ CDN }
    clients={ home_data.clients }
    data={ testimonials }
    PROJECT={ PROJECT }
/>
<Footer />
