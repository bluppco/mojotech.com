---

// IMPORTS JSX COMPONENTS
import Work from "@/components/home/work/jsx/index.jsx"

// GET ASTRO PROPS
const {

    carousel_one,
    carousel_two,
    carousel_three,
    CDN,
    PROJECT

} = Astro.props
---
<Work
    carousel_one={ carousel_one }
    carousel_three={ carousel_three }
    carousel_two={ carousel_two }
    CDN={ CDN }
    client:load
    PROJECT={ PROJECT }
/>
