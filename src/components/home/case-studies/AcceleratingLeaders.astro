---
// IMPORT ASTRO ATOMS
import H2 from "../../../atoms/H2.astro"

// IMPORT ASTRO COMPONENTS
import CaseStudy from "./CaseStudy.astro"

const data = Array(4).fill({

	src: "",
	alt: "",
	title: "Finance & Insurance",
	description: "MojoTech solves the digital challenges plaguing banks and financial institutions with custom fintech solutions including Know Your Customer, cybersecurity, and compliance.",
	href: "",
	button_title: "Credit Karma Case Study",
	aria_label: ""


})
---

<section class="space-y-20">
	<H2>Accelerating Industry Leaders.</H2>
	<div class="grid grid-cols-2 gap-8">
		{

			data.map( ( item ) => {

				return(
					<CaseStudy
						src={ item.src }
						alt={ item.alt }
						title={ item.title }
						description={ item.description }
						href={ item.href }
						button_title={ item.button_title }
						aria_label={ item.aria_label }
					/>
				)

			})

		}
	</div>
</section>
